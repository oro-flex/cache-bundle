{
    "name": "oro-flex/cache-bundle",
    "description": "OroCacheBundle introduces the configuration of the application data cache storage used by application bundles for different cache types.",
    "homepage": "https://github.com/oroinc/platform/tree/master/src/Oro/Bundle/CacheBundle",
    "license": "MIT",
    "authors": [
	    {
            "name": "Nikita Makarov",
            "email": "mesaverde228@gmail.com"
        },
        {
            "name": "Oro, Inc",
            "homepage": "https://oroinc.com/"
        }
    ],
    "require": {
        "php": "~7.4.14 || ~8.0.0",
        "oro-flex/config": "4.2.*",
        "symfony/serializer": "~4.4.24",
        "doctrine/cache": "^1.11 || ^2.0"
    },
    "conflict": {
        "oro/platform": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.4.*"
        },
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Oro\\Bundle\\CacheBundle\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    }
}
